{
  "name": "ngx-addons",
  "version": "0.0.0",
  "packageManager": "pnpm@9.15.9",
  "scripts": {
    "build:omni-auth-core:prd": "ng build omni-auth-core",
    "build:omni-auth-cognito:prd": "ng build omni-auth-cognito",
    "build:omni-auth-ui-material:prd": "ng build omni-auth-ui-material",
    "build:prd": "pnpm run build:omni-auth-core:prd && pnpm run build:omni-auth-cognito:prd && pnpm run build:omni-auth-ui-material:prd",
    "publish:omni-auth-core": "cd dist/omni-auth-core && npm publish && cd ~",
    "publish:omni-auth-core:ci": "cd dist/omni-auth-core && npm publish --provenance && cd ~",
    "publish:omni-auth-ui-material": "cd dist/omni-auth-ui-material && npm publish && cd ~",
    "publish:omni-auth-ui-material:ci": "cd dist/omni-auth-ui-material && npm publish --provenance && cd ~",
    "publish:omni-auth-cognito": "cd dist/omni-auth-cognito && npm publish && cd ~",
    "publish:omni-auth-cognito:ci": "cd dist/omni-auth-cognito && npm publish --provenance && cd ~",
    "tests:headless:coverage": "ng test --no-watch --no-progress --browsers=ChromeHeadless --code-coverage omni-auth-core",
    "tests:headless": "ng test --no-watch --no-progress --browsers=ChromeHeadless omni-auth-core",
    "lint": "ng lint",
    "publish": "pnpm run publish:omni-auth-core && pnpm run publish:omni-auth-ui-material && pnpm run publish:omni-auth-cognito",
    "publish:ci": "pnpm run publish:omni-auth-core:ci && pnpm run publish:omni-auth-ui-material:ci && pnpm run publish:omni-auth-cognito:ci"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "20.1.0",
    "@angular/common": "^20.1.4",
    "@angular/compiler": "^20.1.4",
    "@angular/core": "^20.1.4",
    "@angular/forms": "^20.1.4",
    "@angular/material": "20.1.0",
    "@angular/platform-browser": "^20.1.4",
    "@angular/router": "^20.1.4",
    "@ng-doc/app": "20.1.0",
    "@ng-doc/builder": "20.1.0",
    "@ng-doc/core": "20.1.0",
    "@ng-doc/ui-kit": "20.1.0",
    "aws-amplify": "^6.15.5",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.2.1",
    "@angular-eslint/builder": "20.1.1",
    "@angular/animations": "^20.1.4",
    "@angular/build": "^20.1.4",
    "@angular/cli": "^20.1.4",
    "@angular/compiler-cli": "^20.1.4",
    "@eslint/js": "^9.32.0",
    "@types/jasmine": "~5.1.8",
    "angular-eslint": "20.1.1",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "jasmine-core": "~5.8.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-mocks": "^14.13.5",
    "ng-packagr": "^20.1.0",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "8.34.1"
  }
}
